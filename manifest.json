{
    "name": "Bin Day Reminder",
    "version": "1.1",
    "manifest_version": 2,
    "description": "Shows The Next Bin To Be Collected.",
    "permissions": ["storage", "alarms"],
    "browser_action": {
        "default_title": "Bin Day",
        "default_popup": "popup.html"
    },
    "icons": { "128": "logo.png" },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    }
}